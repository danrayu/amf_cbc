<?xml version="1.0" encoding="utf-8"?>
<data_area>
  <header_data>
    {% if data.app_id %}
      <app_id>{{ data.app_id }}</app_id>
    {% endif %}
    <user_id>{{ data.user_id }}</user_id>
    <user_pwd>{{ data.user_pwd }}</user_pwd>
    <cus_id>{{ data.cus_id }}</cus_id>
    <single_joint>0</single_joint>
    <pre_qual>0</pre_qual>
    <action>XPN</action>
  </header_data>
  <applicant_data>
    <applicant type="primary">
      <person_name>
        <first_name>{{ data.fname }}</first_name>
        {% if data.mname %}
          <middle_name>{{ data.mname }}</middle_name>
        {% endif %}
        <last_name>{{ data.lname }}</last_name>
      </person_name>
      <address_data>
        <address type="current">
          <line_one>{{ data.add_str }}</line_one>
          <city>{{ data.add_city }}</city>
          <state_or_province>{{ data.add_state }}</state_or_province>
          <postal_code>{{ data.add_pcode }}</postal_code>
        </address>
      </address_data>
      <social>{{ data.ssn }}</social>
    </applicant>
  </applicant_data>
</data_area>
